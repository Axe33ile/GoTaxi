<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gotaxi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoTaxi">
        <activity
            android:name=".Navigation.WelcomeSession"
            android:exported="false" />
        <activity
            android:name=".Login_Register.Register"
            android:exported="false" />
        <activity
            android:name=".Login_Register.Login"
            android:exported="false" />
        <activity
            android:name=".Navigation.MainActivity1"
            android:exported="true"
            android:usesCleartextTraffic="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>